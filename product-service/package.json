{
  "name": "product-service",
  "version": "1.5.0",
  "description": "API-based microservices for products",
  "main": "serverless.ts",
  "scripts": {
    "invoke:local:get-products-list": "npx sls invoke local -f getProductsList",
    "invoke:local:get-products-by-id": "npx sls invoke local -f getProductsById --path src/functions/get-products-by-id/mock.json",
    "invoke:local:post-products": "npx sls invoke local -f postProducts --path src/functions/post-products/mock.json",
    "test": "jest",
    "info": "npx serverless info --verbose",
    "deploy": "npx serverless deploy",
    "swagger": "rm -rf swagger-api.json && npx serverless downloadDocumentation --outputFileName=swagger-api.json",
    "release": "npm run test && npm run deploy && npm run swagger"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^1.5.2",
    "@middy/http-json-body-parser": "^1.5.2",
    "dotenv": "^10.0.0",
    "jsonschema": "^1.4.0",
    "pg": "^8.7.1",
    "source-map-support": "^0.5.20"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.59.0",
    "@types/aws-lambda": "^8.10.83",
    "@types/jest": "^27.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.20",
    "@types/pg": "^8.6.1",
    "jest": "^27.2.4",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^2.60.3",
    "serverless-aws-documentation": "^1.1.0",
    "serverless-webpack": "^5.5.4",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.11.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.4.3",
    "webpack": "^5.56.0",
    "webpack-node-externals": "^2.5.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
