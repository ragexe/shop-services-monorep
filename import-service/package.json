{
  "name": "import-service",
  "version": "1.2.1",
  "description": "API-based microservice for importing products",
  "main": "serverless.ts",
  "scripts": {
    "invoke:local:import-products-file": "npx sls invoke local -f importProductsFile --path src/functions/import-products-file/mock.json",
    "invoke:local:import-file-parser": "npx sls invoke local -f importFileParser --path src/functions/import-file-parser/mock.json",
    "test": "jest",
    "deploy": "npx serverless deploy",
    "bump-version:patch": "npm version patch",
    "bump-version:minor": "npm version minor",
    "bump-version:major": "npm version major",
    "bump-version:patch:no-tag": "npm version patch --no-git-tag-version",
    "bump-version:minor:no-tag": "npm version minor --no-git-tag-version",
    "bump-version:major:no-tag": "npm version major --no-git-tag-version",
    "release:patch": "npm run test && npm run bump-version:patch && npm run deploy",
    "release:minor": "npm run test && npm run bump-version:minor && npm run deploy",
    "release:major": "npm run test && npm run bump-version:major && npm run deploy"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^1.5.2",
    "@middy/http-json-body-parser": "^1.5.2",
    "aws-sdk": "^2.987.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^10.0.0",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.55.0",
    "@types/aws-lambda": "^8.10.83",
    "@types/jest": "^27.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.15",
    "aws-sdk-mock": "^5.3.0",
    "jest": "^27.2.0",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^2.23.0",
    "serverless-webpack": "^5.3.5",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.4.3",
    "webpack": "^5.20.2",
    "webpack-node-externals": "^2.5.2"
  },
  "author": "ragexe",
  "license": "MIT"
}
